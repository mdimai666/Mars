@if (OverrideComponent is not null)
{
    <DynamicComponent Type="OverrideComponent" />
}
else
{
    <div class="misc-inner p-2 p-sm-5 h-100 w-100 xcenter">
        <div class="w-100 text-center">
            <h1 class="display-1 text-accent fw-bold">404</h1>
            <h2 class="mb-1">Страница не найдена </h2>
            <a class="mb-2 btn btn-primary btn-sm-block " href="/">
                На Главную
            </a>

        </div>
    </div>
}

@code {
    public static Type? OverrideComponent { get; set; }
}
