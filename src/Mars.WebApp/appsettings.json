{
    "ConnectionStrings": {
        "DefaultConnection": "Host=127.0.0.1;Database=mars;Username=mars;Password=mars"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "Quartz.Impl.StdSchedulerFactory": "Warning",
            "Quartz.Core.QuartzScheduler": "Warning"
        }
    },
    "Urls": "http://+:80",
    "AllowedHosts": "*",
    "JWTSettings": {
        "PrivateKeyPath": "jwt_private.pem",
        "validAudience": "mars-app",
        "expiryInMinutes": 43200
    },
    "AppFront": [
        {
            "_allow_Mode": "None,HandlebarsTemplate,HandlebarsTemplateStatic,ServeStaticBlazor,BlazorPrerender",
            "Mode": "None",
            "Path": "",
            "Url": ""
        }
    ],
    "Plugins": {
        "_HelloPlugin1": {
            "ContentRootPath": "...",
            "AssemblyPath": "...\\HelloPlugin1.dll"
        }
    },
    "FeatureManagement": {
        "DockerAgent": false,
        "AITool": true,
        "SingleSignOn": true,
        "MonacoRoslynCompletionProvider": false
    },
    "AppDatabaseMigrationOptions": {
        "AutoMigrate": true
    },
    "OpenTelemetryAppOptions": {
        "Enable": false,
        "AddProcessInstrumentation": true,
        "AddRuntimeInstrumentation": false,
        "AddEntityFrameworkCoreInstrumentation": false,
        "AddPrometheusExporter": true,
        "PrometheusScrapingEndpoint": "/_ot/metrics"
    }
}
